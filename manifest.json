{
  "name": "git-praise",
  "description": "Wholesome revision history for teams. Replace the blame button with a praise button!",
  "version": "0.1.3",
  "icons": {
    "16": "icons/gp-icon-x16.png",
    "48": "icons/gp-icon-x48.png",
    "128": "icons/gp-icon-x128.png"
  },
  "manifest_version": 2,
  "default_locale": "en",
  "permissions": [
    "tabs",
    "*://github.com/*",
    "*://www.github.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
          "*://github.com/*",
          "*://www.github.com/*"
      ],
      "js": [
          "js/jquery.js",
          "js/inject.js"
      ]
    }
  ],
  "background": {
    "scripts": [
        "js/background.js"
    ]
  }
}
